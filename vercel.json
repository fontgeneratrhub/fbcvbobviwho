{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "frontend/dist",
      "use": "@vercel/static-build"
    }
  ],
  "rewrties": [
    {
      "src": "/(.*)",
      "dest": "backend/main.js"
    }
  ]
}
